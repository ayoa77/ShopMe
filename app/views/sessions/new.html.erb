<h1>Sign In#</h1>

<%= form_for :sessions do |f| %>
  <div class = "field">
    <%= f.label :email %>
    <%= f.text_field :email %>
  </div>
  <div class = "field">
    <%= f.label :password %>
    <%= f.password_field :password %>
  </div>
  <div class="submit-field">
      <%= f.submit %>
  </div>
  <% end %>
